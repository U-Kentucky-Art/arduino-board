void setup() {
  pinMode(LED_BUILTIN, OUTPUT);
  pinMode(6, OUTPUT);
  pinMode(7, OUTPUT);
}


void loop() {
  digitalWrite(LED_BUILTIN, HIGH);   
  digitalWrite(6, HIGH);   
  digitalWrite(7, HIGH);   
  delay(1000);                       
  digitalWrite(LED_BUILTIN, LOW); 
  digitalWrite(6, LOW); 
  digitalWrite(7, LOW); 
  delay(1000);                      
}
